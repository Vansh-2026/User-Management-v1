{
  "name": "@progress/kendo-angular-dateinputs",
  "version": "22.0.1",
  "description": "Kendo UI for Angular Date Inputs Package - Everything you need to add date selection functionality to apps (DatePicker, TimePicker, DateInput, DateRangePicker, DateTimePicker, Calendar, and MultiViewCalendar).",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Progress",
  "homepage": "https://www.telerik.com/kendo-angular-ui/components/",
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "keywords": [
    "Angular",
    "Kendo UI",
    "angular dateinput",
    "angularjs",
    "calendar",
    "date-input",
    "date-picker",
    "date-time",
    "dateinput",
    "dateinput for angular",
    "daterange",
    "datetimepicker",
    "multiviewcalendar",
    "timepicker"
  ],
  "@progress": {
    "friendlyName": "DateInputs",
    "migrations": {
      "options": {
        "parser": "tsx",
        "pattern": "*.{ts,html}"
      },
      "codemods": {
        "19": [
          {
            "description": "Migrate subtitle to adaptiveSubtitle for datepicker",
            "file": "codemods/v19/datepicker-subtitle.js",
            "prompt": "true"
          },
          {
            "description": "Migrate title to adaptiveTitle for datepicker",
            "file": "codemods/v19/datepicker-title.js",
            "prompt": "true"
          },
          {
            "description": "Migrate subtitle to adaptiveSubtitle for daterange-popup",
            "file": "codemods/v19/daterange-popup-subtitle.js",
            "prompt": "true"
          },
          {
            "description": "Migrate title to adaptiveTitle for daterange-popup",
            "file": "codemods/v19/daterange-popup-title.js",
            "prompt": "true"
          },
          {
            "description": "Migrate subtitle to adaptiveSubtitle for datetimepicker",
            "file": "codemods/v19/datetimepicker-subtitle.js",
            "prompt": "true"
          },
          {
            "description": "Migrate title to adaptiveTitle for datetimepicker",
            "file": "codemods/v19/datetimepicker-title.js",
            "prompt": "true"
          },
          {
            "description": "Migrate subtitle to adaptiveSubtitle for timepicker",
            "file": "codemods/v19/timepicker-subtitle.js",
            "prompt": "true"
          },
          {
            "description": "Migrate title to adaptiveTitle for timepicker",
            "file": "codemods/v19/timepicker-title.js",
            "prompt": "true"
          }
        ]
      }
    },
    "package": {
      "productName": "Kendo UI for Angular",
      "productCode": "KENDOUIANGULAR",
      "publishDate": 1768481895,
      "licensingDocsUrl": "https://www.telerik.com/kendo-angular-ui/my-license/"
    }
  },
  "peerDependencies": {
    "@angular/animations": "19 - 21",
    "@angular/common": "19 - 21",
    "@angular/core": "19 - 21",
    "@angular/forms": "19 - 21",
    "@angular/platform-browser": "19 - 21",
    "@progress/kendo-licensing": "^1.7.0",
    "@progress/kendo-angular-buttons": "22.0.1",
    "@progress/kendo-angular-common": "22.0.1",
    "@progress/kendo-angular-utils": "22.0.1",
    "@progress/kendo-angular-intl": "22.0.1",
    "@progress/kendo-angular-l10n": "22.0.1",
    "@progress/kendo-angular-icons": "22.0.1",
    "@progress/kendo-angular-popup": "22.0.1",
    "@progress/kendo-angular-navigation": "22.0.1",
    "rxjs": "^6.5.3 || ^7.0.0"
  },
  "dependencies": {
    "tslib": "^2.3.1",
    "@progress/kendo-angular-schematics": "22.0.1",
    "@progress/kendo-common": "^1.0.1",
    "@progress/kendo-date-math": "^1.1.0",
    "@progress/kendo-dateinputs-common": "^0.4.7"
  },
  "schematics": "./schematics/collection.json",
  "module": "fesm2022/progress-kendo-angular-dateinputs.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "default": "./fesm2022/progress-kendo-angular-dateinputs.mjs"
    }
  },
  "sideEffects": false
